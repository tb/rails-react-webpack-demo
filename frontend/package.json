{
  "name": "rails-react-webpack-demo",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ../public/webpack",
    "start": "npm run build:dev",
    "build:dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js --inline --hot",
    "build:prod": "npm run clean && NODE_ENV=production webpack --config webpack.config.js"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-rails-ujs": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.11.2",
    "postcss-loader": "^1.1.0",
    "sass-loader": "^4.0.2",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
